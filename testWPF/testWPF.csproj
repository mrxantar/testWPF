  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\Montserrat-Italic-VariableFont_wght.ttf" />
    <Resource Include="Fonts\Montserrat-VariableFont_wght.ttf" />
  </ItemGroup>

</Project>
